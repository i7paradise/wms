<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity CompanyContainer.
    -->
    <changeSet id="20220204215808-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="container_category_id"
                                 baseTableName="company_container"
                                 constraintName="fk_company_container__container_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="container_category"/>

        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="company_container"
                                 constraintName="fk_company_container__company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>
    </changeSet>
</databaseChangeLog>
